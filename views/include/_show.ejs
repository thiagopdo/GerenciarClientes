<% for(let i=0; i < users.length; i++) {%>
<tr>
  <td><%= i + 1 %></td>
  <td><%= users[i].name %></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].birthday %></td>
  <td><%= users[i].cpf %></td>
  <td><%= users[i].status %></td>
  <td>
    <a href="/update-user?id=<%= users[i]._id %>" class="btn border-shadow update">
      <span class="text-gradient"><i class="fa-solid fa-user-pen"></i></span>
    </a>
    <a class="btn border-shadow delete" data-id=<%= users[i]._id %>>
      <span class="text-gradient"><i class="fa-solid fa-user-xmark"></i></span>
    </a>
  </td>
</tr>
<% } %>